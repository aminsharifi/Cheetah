@page "/"
@page "/Contact"
@inherits MyComponentBase
@inject IJSRuntime JSRuntime;
@using BlazorServerResource = Cheetah.Presentation.Web.Blazor.Server.Resx.Localization

<PageTitle>@(globalization.GetValue(nameof(BlazorServerResource.BlazorServer_NewMessage)))</PageTitle>

<div class="container-fluid" dir="rtl">
    <h1>@(globalization.GetValue(nameof(BlazorServerResource.BlazorServer_Suggestion)))</h1>
    <br />

    <div class="row px-3">
        <div class="p-3">
            <img src="cheetah.jpg" />
        </div>
    </div>
    <br />
    <div class="row px-3">
        <CInput CDescription="@(globalization.GetValue(nameof(BlazorServerResource.BlazorServer_NameAndFamily) ))"
                CDisplayName="@(globalization.GetValue(nameof(BlazorServerResource.BlazorServer_NameAndFamily) ))"
                CFieldType="@(Core.Enums.FieldType.CString)"></CInput>

        <CInput CDescription="@(globalization.GetValue(nameof(BlazorServerResource.BlazorServer_Email) ))"
                CDisplayName="@(globalization.GetValue(nameof(BlazorServerResource.BlazorServer_Email) ))"
                CFieldType="@(Core.Enums.FieldType.CString)"></CInput>

        <CInput CDescription="@(globalization.GetValue(nameof(BlazorServerResource.BlazorServer_Message) ))"
                CDisplayName="@(globalization.GetValue(nameof(BlazorServerResource.BlazorServer_Message) ))"
                CFieldType="@(Core.Enums.FieldType.CString)"></CInput>
    </div>
</div>


