@inherits SharedPage

@if (Name == nameof(F_Scenario))
{
    <CUpsert_Scenario Name="@Name" IsInline="@IsInline"
                  ParentCallback="@ParentCallback" Id="@Id"
                  reference="@reference" LoadData="@LoadData"
                  Title="@Title">
    </CUpsert_Scenario>
}

else if (Name == nameof(D_Location))
{
    <CUpsert_Location Name="@Name" IsInline="@IsInline"
                  ParentCallback="@ParentCallback" Id="@Id"
                  reference="@reference" LoadData="@LoadData"
                  Title="@Title">
    </CUpsert_Location>
}
else if (Name == nameof(D_Tag))
{
    <CUpsert_Tag Name="@Name" IsInline="@IsInline"
                 ParentCallback="@ParentCallback" Id="@Id"
                 reference="@reference" LoadData="@LoadData"
                 Title="@Title">
    </CUpsert_Tag>
}
else if (Name == nameof(D_Process))
{
    <CUpsert_Process Name="@Name" IsInline="@IsInline"
                 ParentCallback="@ParentCallback" Id="@Id"
                 reference="@reference" LoadData="@LoadData"
                 Title="@Title">
    </CUpsert_Process>
}
else if (Name == nameof(D_User))
{
    <CUpsert_User Name="@Name" IsInline="@IsInline"
              ParentCallback="@ParentCallback" Id="@Id"
              reference="@reference" LoadData="@LoadData"
              Title="@Title">
    </CUpsert_User>
}
else if (Name == nameof(F_Condition))
{
    <CUpsert_Condition Name="@Name" IsInline="@IsInline"
                  ParentCallback="@ParentCallback" Id="@Id"
                  reference="@reference" LoadData="@LoadData"
                  Title="@Title">
    </CUpsert_Condition>
}
else
{
    <CUpsert_General Name="@Name" IsInline="@IsInline"
                 ParentCallback="@ParentCallback" Id="@Id"
                 reference="@reference" LoadData="@LoadData"
                 Title="@Title">
    </CUpsert_General>
}
