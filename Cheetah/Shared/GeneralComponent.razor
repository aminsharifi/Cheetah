                              ParentCallback="@ParentCallback" Id="@Id"
                              reference="@reference" LoadData="@LoadData"
                              Title="@Title">
    </CUpsert_D_Strategy>
}
else if (Name == nameof(D_Location))
{
    <CUpsert_D_Location Name="@Name" IsInline="@IsInline"
                    ParentCallback="@ParentCallback" Id="@Id"
                    reference="@reference" LoadData="@LoadData"
                    Title="@Title">
    </CUpsert_D_Location>
}
else if (Name == nameof(CUpsert_D_TagType))
{
    <CUpsert_D_TagType Name="@Name" IsInline="@IsInline"
                   ParentCallback="@ParentCallback" Id="@Id"
                   reference="@reference" LoadData="@LoadData"
