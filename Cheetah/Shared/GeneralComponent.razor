else if (Name == nameof(D_User))
{
    <CUpsert_D_User Name="@Name" IsInline="@IsInline"
                ParentCallback="@ParentCallback" Id="@Id"
                reference="@reference" LoadData="@LoadData"
                Title="@Title">
    </CUpsert_D_User>
}
else
{
    <CUpsert_D_General Name="@Name" IsInline="@IsInline"
                   ParentCallback="@ParentCallback" Id="@Id"
                   reference="@reference" LoadData="@LoadData"
                   Title="@Title">
    </CUpsert_D_General>
}
