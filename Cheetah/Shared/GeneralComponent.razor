{
    <CUpsert_D_Position Name="@Name" IsInline="@IsInline"
                    ParentCallback="@ParentCallback" Id="@Id"
                    reference="@reference" LoadData="@LoadData"
                    Title="@Title">
    </CUpsert_D_Position>
}
else if (Name == nameof(D_Process))
{
